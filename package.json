{
  "name": "@zaguan/sdk",
  "version": "1.1.0",
  "description": "Official ZaguÃ¡n SDK for TypeScript",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "example:basic": "node dist/examples/basic-chat.js",
    "example:streaming": "node dist/examples/streaming-chat.js",
    "example:multi": "node dist/examples/multi-provider.js",
    "example:credits": "node dist/examples/credits-usage.js",
    "example:functions": "node dist/examples/function-calling.js",
    "example:vision": "node dist/examples/vision-multimodal.js",
    "example:providers": "node dist/examples/provider-specific.js",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZaguanLabs/zaguan-sdk-ts.git"
  },
  "keywords": [
    "zaguan",
    "ai",
    "sdk",
    "openai",
    "llm",
    "typescript"
  ],
  "author": "Zaguan Labs",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/ZaguanLabs/zaguan-sdk-ts/issues"
  },
  "homepage": "https://github.com/ZaguanLabs/zaguan-sdk-ts#readme",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^4.0.10",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
